{
  "name": "frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 5173",
    "build": "vite build",
    "preview": "vite preview --host 0.0.0.0 --port 5173",
    "gen:types:local": "openapi-typescript http://localhost:8000/api/schema/ -o src/api/__generated__/vims-types.ts",
    "gen:schema": "docker compose exec web python src/manage.py spectacular --format openapi-json --file /app/openapi.json",
    "pull:schema": "docker cp $(docker compose ps -q web):/app/openapi.json ./openapi.json",
    "gen:types:file": "openapi-typescript ./openapi.json -o src/api/__generated__/vims-types.ts",
    "gen:types:offline": "npm run gen:schema && npm run pull:schema && npm run gen:types:file",
    "prebuild": "npm run gen:types:local"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.2",
    "@mui/material": "^7.3.2",
    "@mui/x-data-grid": "^8.11.2",
    "axios": "^1.12.1",
    "notistack": "^3.0.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.9.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.4",
    "openapi-typescript": "^7.9.1",
    "typescript": "^5.6.2",
    "vite": "^5.4.10"
  }
}
